{
  "name": "thunderbench",
  "version": "1.0.9",
  "description": "ThunderBench - 高性能API性能测试工具核心引擎",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "bin/",
    "examples/",
    "docs/",
    "scripts/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npx tsup && npx tsc --project tsconfig.build.json",
    "dev": "npx tsx watch src/index.ts",
    "test": "vitest --run",
    "release": "npm run build && npm run test && npx bumpp && npm publish --access=public"
  },
  "keywords": [
    "performance",
    "benchmark",
    "api-testing",
    "wrk",
    "load-testing",
    "stress-testing",
    "nodejs",
    "typescript"
  ],
  "author": "ThunderBench Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thunderbench/thunderbench.git"
  },
  "homepage": "https://github.com/thunderbench/thunderbench#readme",
  "bugs": {
    "url": "https://github.com/thunderbench/thunderbench/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "ora": "^7.0.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@hono/node-server": "^1.19.7",
    "@sinclair/typebox": "0.34.39",
    "@types/express": "^5.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "bumpp": "^10.2.3",
    "bun-types": "latest",
    "elysia": "^1.4.19",
    "express": "^5.2.1",
    "hono": "^4.11.3",
    "tsdown": "^0.19.0-beta.4",
    "typescript": "^5.9.3",
    "vafast": "^0.3.4",
    "vitest": "^1.0.0"
  }
}
